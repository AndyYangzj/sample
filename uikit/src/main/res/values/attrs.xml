        <attr name="inner_arc_progress_bar_width" format="dimension" />
        <attr name="inner_arc_progress_bar_height" format="dimension" />
        <attr name="tick_width" format="dimension" />
        <attr name="tick_height" format="dimension" />
        <attr name="tick_margin_top" format="dimension" />
        <attr name="tick_margin_bottom" format="dimension" />
        <attr name="tick_font_size" format="dimension" />
        <attr name="tick_font_margin" format="dimension" />
        <attr name="tick_font_color" format="color" />
    </declare-styleable>
    <declare-styleable name="arc_progress_bar">
        <attr name="angel_start" format="integer" />
        <attr name="angel_end" format="integer" />
        <attr name="stroke_width" format="dimension" />
        <attr name="center_font_size" format="dimension" />
        <attr name="center_font_color" format="color" />
        <attr name="below_font_size" format="dimension" />
        <attr name="below_font_color" format="color" />
        <attr name="bottom_font_size" format="dimension" />
        <attr name="bottom_font_color" format="color" />
        <attr name="all_size" format="dimension" />
    </declare-styleable>
</resources>
